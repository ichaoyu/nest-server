import {
  Column,
  CreateDateColumn,
  Entity,
  PrimaryGeneratedColumn,
  UpdateDateColumn,
} from 'typeorm';

/**
 * 表结构
 */
@Entity({ name: '{{name}}' })
export class {{UpName}}Entity {
  /**
   * 主键
   */
  @PrimaryGeneratedColumn({ name: 'id', type: 'bigint' })
  id: string;

  /**
   * demo名称
   */
  @Column({ name: 'title' })
  title: string;

  /**
   * 创建时间
   */
  @CreateDateColumn({ name: 'create_time' })
  createTime: Date;

  /**
   * 更新时间
   */
  @UpdateDateColumn({ name: 'update_time', default: () => 'CURRENT_TIMESTAMP' })
  updateTime: Date;
}
